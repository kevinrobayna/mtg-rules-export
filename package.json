{
  "name": "javascript-action",
  "version": "1.0.0",
  "description": "Github Action to export the rules from the website of the game Magic: The Gathering",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint .",
    "compile": "ncc build src/index.js -o dist --source-map --license licenses.txt",
    "test": "jest src",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "all": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kevinrobayna/mtg-rules-export.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript"
  ],
  "author": "Kevin Robayna",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevinrobayna/mtg-rules-export/issues"
  },
  "homepage": "https://github.com/kevinrobayna/mtg-rules-export#readme",
  "dependencies": {
    "@actions/core": "^1.9.0",
    "cheerio": "^1.0.0-rc.12",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.34.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.19.0",
    "jest": "^28.1.1"
  }
}
